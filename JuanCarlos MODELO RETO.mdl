{UTF-8}
area staging=
	DELAY FIXED(Productos a staging, 7/30 , 0 )
	~	
	~		|

Capacidad total CAT=
	24*64*30*2
	~	
	~	Capacidad en toneladas del CAT
	|

Capacidad total Tiendas=
	24*64*30*2
	~	
	~		|

Productos en Espera= INTEG (
	Productos Ordenados-Productos a staging,
		Capacidad total CAT)
	~	
	~	Toneladas
	|

Demanda No Satisfecha=
	IF THEN ELSE(Productos en Tienda<=Punto de reorden, Capacidad total Tiendas-Punto de reorden\
		, Consumo )
	~	
	~	Producto debe ser igual al punto de reorden
	|

Productos Entregados=
	DELAY FIXED(Productos a camion*Flujo vehicular, 1/30 , 0 )
	~	
	~		|

Productos Ordenados=
	DELAY FIXED(Demanda No Satisfecha*Flujo vehicular, 7/30,0)
	~	
	~		|

Punto de reorden=
	24*64*30
	~	
	~	Mitad de la capacidad de las tiendas en toneladas
	|

Productos a staging=
	DELAY FIXED(MIN(Productos en Espera,Eficiencia Empleados), 1/30 , 0 )
	~	
	~		|

Productos a camion=
	DELAY FIXED(MIN(Eficiencia Empleados,productos cargados), 1/30 , 0 )
	~	
	~		|

productos cargados= INTEG (
	area staging-Productos a camion,
		Capacidad total CAT)
	~	
	~		|

Productos en Tienda= INTEG (
	Productos Entregados-Consumo,
		Capacidad total Tiendas)
	~	
	~		|

Empleados=
	1000
	~	
	~		|

Flujo vehicular = WITH LOOKUP (
	Time,
		   ([(0,0)-(24,24)],(0,0.7),(1,0.7),(2,0.8),(3,1),(4,0.9),(5,1),(6,0.7),(7,0.5),(8,\
		0.5),(9,0.7),(10,0.8),(11,0.6),(12,0.3),(13,0.7),(14,0.8),(15,1),(16,0.9),(17,1),(18\
		,0.5),(19,0.7),(20,0.6),(21,0.8),(22,0.8),(23,0.5),(24,0.3) ))
	~	
	~		|

Movimientos promedio=
	135*2*30
	~	
	~		|

Eficiencia Empleados=
	ABS(tasa de rotacion-1)*Empleados*(Movimientos promedio/1.5)
	~	
	~	Cuantas toneladas mueven por mes
	|

tasa de rotacion = WITH LOOKUP (
	Time,
		     ([(0,0)-(10,10)],(0,0.1),(1,0.1),(5,0.28),(6,0.3),(7,0.28),(11,0.02),(12,0.001\
		),(13,0.02),(17,0.28),(18,0.3),(19,0.28),(24,0.001),(30,0.3) ))
	~	
	~		|

Consumo=
	DELAY FIXED(Productos en Tienda*Ventas Estacionales, 15/30,0)
	~	
	~		|

Ventas Estacionales = WITH LOOKUP (
	Time,
		       ([(0,0)-(24,1.2)],(0,1),(1,1),(2,0.8),(3,0.6),(4,0.8),(5,0.7),(6,0.7),(7,0.8\
		),(8,0.8),(9,0.9),(10,0.8),(11,1),(12,1),(13,0.9),(14,0.5),(15,0.7),(16,0.6),(17,0.5\
		),(18,0.8),(19,0.8),(20,0.6),(21,0.7),(22,0.9),(23,1),(24,1) ))
	~	
	~		|

