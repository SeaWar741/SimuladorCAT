Productos Cargados=
	MIN(Numero de socios que descargan, Tarimas Disponibles)
	~	
	~		|

Productos Descargados=
	Numero de Camiones que Llegan/Numero de socios que descargan
	~	
	~		|

Nuevos Pedidos=
	(Necesidad de tiendas)*Trafico
	~	
	~		|

Trafico = WITH LOOKUP (
	Time,
		 ([(0,0)-(24,1.5)],(0.380091,1.15347),(1.46606,1.05446),(2.60633,1.14356),(3.42081,\
		1.0396),(4.83258,1.11386),(5.80995,1.00495),(7.27602,1.13861),(8.95928,1.0297),(10.2081\
