{UTF-8}
Capacidad total CAT=
	24*64*30*2
	~	
	~	Capacidad en toneladas del CAT
	|

Capacidad total tiendas=
	24*64*30*2
	~	
	~		|

Lote en tienda= INTEG (
	Lote en entrega-Lote de ventas,
		Capacidad total tiendas)
	~	
	~		|

Lote entrante=
	DELAY FIXED(Lote de produccion*Flujo vehicular, 7/30,0)
	~	
	~		|

Lote a staging=
	DELAY FIXED(MIN(Lote en espera,Eficiencia Empleados), 1/30 , 0 )
	~	
	~		|

Lote de envio= INTEG (
	Lote de staging-Lote procesado,
		Capacidad total CAT)
	~	
	~		|

Lote de produccion=
	IF THEN ELSE(Lote en tienda<=Punto de reorden, Capacidad total tiendas-Punto de reorden\
		, Lote de ventas )
	~	
	~	Producto debe ser igual al punto de reorden
	|

Lote de staging=
	DELAY FIXED(Lote a staging, 7/30 , 0 )
	~	
	~		|

Punto de reorden=
	24*64*30
	~	
	~	Mitad de la capacidad de las tiendas en toneladas
	|

Lote en entrega=
	DELAY FIXED(Lote procesado*Flujo vehicular, 1/30 , 0 )
	~	
	~		|

Lote en espera= INTEG (
	Lote entrante-Lote a staging,
		Capacidad total CAT)
	~	
	~	Toneladas
	|

Lote procesado=
	DELAY FIXED(MIN(Eficiencia Empleados,Lote de envio), 1/30 , 0 )
	~	
	~		|

Número de socios=
	1000
	~	
	~		|

Flujo vehicular = WITH LOOKUP (
	Time,
		   ([(0,0)-(24,24)],(0,0.7),(1,0.7),(2,0.8),(3,1),(4,0.9),(5,1),(6,0.7),(7,0.5),(8,\
		0.5),(9,0.7),(10,0.8),(11,0.6),(12,0.3),(13,0.7),(14,0.8),(15,1),(16,0.9),(17,1),(18\
		,0.5),(19,0.7),(20,0.6),(21,0.8),(22,0.8),(23,0.5),(24,0.3) ))
	~	
	~		|

Movimientos promedio=
	135*2*30
	~	
	~		|

Eficiencia Empleados=
	ABS(Tasa de rotación-1)*Número de socios*(Movimientos promedio/1.5)
	~	
	~	Cuantas toneladas mueven por mes
	|

Tasa de rotación = WITH LOOKUP (
	Time,
		     ([(0,0)-(10,10)],(0,0.1),(1,0.1),(5,0.28),(6,0.3),(7,0.28),(11,0.02),(12,0.001\
		),(13,0.02),(17,0.28),(18,0.3),(19,0.28),(24,0.001),(30,0.3) ))
	~	
	~		|

Lote de ventas=
	DELAY FIXED(Lote en tienda*Tasa de venta estacional, 15/30,0)
	~	
	~		|

Tasa de venta estacional = WITH LOOKUP (
	Time,
		       ([(0,0)-(24,1.2)],(0,1),(1,1),(2,0.8),(3,0.6),(4,0.8),(5,0.7),(6,0.7),(7,0.8\
		),(8,0.8),(9,0.9),(10,0.8),(11,1),(12,1),(13,0.9),(14,0.5),(15,0.7),(16,0.6),(17,0.5\
		),(18,0.8),(19,0.8),(20,0.6),(21,0.7),(22,0.9),(23,1),(24,1) ))
	~	
	~		|

